[user]
	name = fugakkbn
	email = aax.chiri@gmail.com
[init]
	defaultBranch = main
[ghq]
	root = ~/projects
[alias]
	sw = switch
	sc = switch -c
	pushf = push --force-with-lease
	st = status
	lp = log -p
	lol = log --graph --decorate --pretty=oneline --all --abbrev-commit
	ba = branch -a
	bd = branch -d
	sh = stash
	sl = stash list
	sp = stash pop
