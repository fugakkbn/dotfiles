[user]
	name = fugakkbn
	email = aax.chiri@gmail.com
[init]
	defaultBranch = main
[ghq]
	root = ~/projects
[alias]
	ca = commit -am
	cm = commit -m
	sw = switch
	sc = switch -c
	ri = rebase -i
	pr = pull --rebase
	pf = push --force-with-lease
	st = status
	lp = log -p
	lol = log --graph --decorate --pretty=oneline --all --abbrev-commit
	ba = branch -a
	bd = branch -d
	su = stash -u
	sl = stash list
	sp = stash pop
